@model IEnumerable<Prueba002.Models.dbModels.Carrito>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProductoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProductoNavigation.NombreProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.Nombre)
            </td>
            <td>
                    @Html.ActionLink("Edit", "Edit", new{id=item.IdProducto,idu=item.IdUsuario}) |
                    @Html.ActionLink("Details", "Details", new{id=item.IdProducto,idu=item.IdUsuario}) |
                    @Html.ActionLink("Delete", "Delete", new{id=item.IdProducto,idu=item.IdUsuario}) |
            </td>
        </tr>
}
    </tbody>
</table>
